{
  "name": "magFlow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/magFlow.umd.js",
  "module": "./dist/magFlow.es.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/magFlow.es.js",
      "require": "./dist/magFlow.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build",
    "build:types": "npx tsc --project tsconfig.lib.json",
    "build:all": "npm run build:types && npm run build:lib",
    "preview": "vite preview",
    "serve:node": "npx http-server public -p 3000 -c-1",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "cesium": "^1.132.0",
    "lil-gui": "^0.20.0",
    "proj4": "^2.19.10"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-cesium": "^1.2.23"
  },
  "peerDependencies": {
    "cesium": "^1.132.0"
  }
}
